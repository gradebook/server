{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "course.cutoffs",
  "description": "Parsed JSON object of all cutoffs in a course",
  "type": "object",
  "minProperties": 4,
  "maxProperties": 12,
  "propertyNames": {
    "enum": [
      "A+",
      "A",
      "A-",
      "B+",
      "B",
      "B-",
      "C+",
      "C",
      "C-",
      "D+",
      "D",
      "D-"
    ]
  },
  "patternProperties": {
    "*": {
      "type": "number",
      "minimum": 10,
      "maximum": 10000
    }
  }
}
